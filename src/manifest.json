{

    "manifest_version": 2,
    "name": "Libify",
    "version": "1.0.1",
  
    "description": "Access papers through library outside the network.",
  
    "icons": {
      "48": "icons/libify.png"
    },
  
    "background": {
        "scripts": [ "libify.js" ]
    },

    "browser_action": {
        "default_icon": {
            "32": "icons/libify.png"
        }
    },

    "permissions": [
        "tabs"
    ],

    "applications": {
        "gecko": {
          "id": "libify@libificationfactory.com"
        }
      }
  
  }
